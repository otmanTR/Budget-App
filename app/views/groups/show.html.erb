
<div class='category_container'>
    <h1>Transections</h1>
 <%@entities.each do |entity|%>
     <div class='group'>
      
        <p class="group-icon"><%= entity.name %></p>
         <p class="date_time"><%= entity.created_at.strftime("%Y-%m-%d") %></p>
 </div>
    <%end%>
    
    
    <div class='group_add'>
        <h2><%= link_to 'Add new entity', new_group_entity_path(group_id:@group.id), class:'group_add_btn' %></h2>
    </div>


 
</div>